<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="dk.lasse_it.smartcontrol">
    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="dk.lasse_it.smartcontrol.Light"
            android:label="@string/light"/>
        <activity
            android:name="dk.lasse_it.smartcontrol.Settings"
            android:label="@string/light"/>
        <activity
            android:name="dk.lasse_it.smartcontrol.Alarmview"
            android:label="@string/light"/>
        <activity
            android:name="dk.lasse_it.smartcontrol.Musicplayer_web"
            android:label="@string/music"
            android:theme="@style/abtheme"/>
        <activity
            android:name="dk.lasse_it.smartcontrol.Main"
            android:label="@string/app_name" >

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity><!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>
    <uses-permission android:name="android.permission.INTERNET" />
</manifest>